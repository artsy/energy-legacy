{
  "name": "energy",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "concurrently 'yarn run relay' 'react-native start'",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "setup:artsy": "./scripts/setup-env-for-artsy",
    "postinstall": "patch-package",
    "sync-schema": "curl https://raw.githubusercontent.com/artsy/metaphysics/main/_schemaV2.graphql -o data/schema.graphql; yarn prettier --write --parser graphql data/schema.graphql",
    "relay": "yarn run relay-compiler --watch"
  },
  "dependencies": {
    "@artsy/palette-tokens": "^2.6.0",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@styled-system/theme-get": "^5.1.2",
    "easy-peasy": "^5.0.4",
    "events": "^3.3.0",
    "formik": "^2.2.9",
    "immer-peasy": "^3.1.3",
    "lodash": "^4.17.21",
    "patch-package": "^6.4.7",
    "qs": "^6.10.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.66.2",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.4.8",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-haptic-feedback": "^1.13.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.9.0",
    "react-native-svg": "^12.1.1",
    "react-relay": "^12.0.0",
    "react-relay-network-modern": "^6.2.1",
    "react-spring": "8.0.23",
    "relay-runtime": "^12.0.0",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.176",
    "@types/qs": "^6.9.7",
    "@types/react-native": "^0.65.0",
    "@types/react-relay": "^11.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/relay-runtime": "^12.0.1",
    "@types/styled-components-react-native": "^5.1.3",
    "@types/styled-system": "^5.1.13",
    "@types/styled-system__theme-get": "^5.0.2",
    "@types/yup": "^0.29.13",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-relay": "^12.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^7.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "graphql": "^15.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.6",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "2.4.1",
    "react-native-flipper": "^0.127.0",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^2.0.1",
