    "relay-compiler-language-typescript": "^15.0.0",
    "relay-config": "^12.0.1",
    "typescript": "^4.5.4",
    "vscode-apollo-relay": "^1.5.2"
  },
  "resolutions": {
    "@types/react": "^17",
    "styled-components": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
